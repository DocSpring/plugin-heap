{
    "name": "gitbook-plugin-heap",
    "description": "Heap tracking for your gitbook",
    "main": "index.js",
    "version": "2.0.1",
    "engines": {
        "gitbook": ">=2.5.1"
    },
    "homepage": "https://github.com/GitbookIO/plugin-ga",
    "repository": {
        "type": "git",
        "url": "https://github.com/GitbookIO/plugin-ga.git"
    },
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/GitbookIO/plugin-ga/issues"
    },
    "gitbook": {
        "properties": {
            "token": {
                "type": "string",
                "title": "Heap Token",
                "required": true
            },
            "configuration": {
                "type": [ "object" ],
                "title": "Configuration",
                "default": {}
            }
        }
    }
}
